function rotate(arr, moveBy) {
  var dest = [];
  var current = moveBy <= 0 ? Math.abs(moveBy) : arr.length - moveBy;

  for (var index = 0; index < arr.length; index++) {
    dest[current] = arr[index];
    current++;
    if (current === arr.length) current = 0;
  }

  return dest;
}

function reverse(src) {
  var dest = [];

  for (var index = src.length - 1; index >= 0; index--) {
    dest.push(src[index]);
  }

  return dest;
}

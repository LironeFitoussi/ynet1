};

exports.analyticsPushFunc = function (action, category, title) {
  window.dataLayer = window.dataLayer || [];
  var eventText = category == "Send Comment" ? "comments_events" : "GA_Events";
  var categoryText = category == "Send Comment" ? "Article" : "Share";
  window.dataLayer.push({
    event: eventText,
    Category: categoryText,
    Action: action,
    Label: title ? title : category
  });
};

var getDateParts = function getDateParts(date, timezone) {
  if (false) {} else {
    return getDatePartsUsingIntl(date, timezone);
  }
};

var getDatePartsUsingIntl = function getDatePartsUsingIntl(date, timezone) {
  var intel = new Intl.DateTimeFormat("en-us", {
    timeZone: timezone,
    hour12: false,
    year: "numeric",
    month: "numeric",
    day: "numeric",
    hour: "numeric",
    minute: "numeric",
    second: "numeric"
  });
  var partsArray = intel.formatToParts(date);
  var parts = {};
  var _iteratorNormalCompletion2 = true;
  var _didIteratorError2 = false;
  var _iteratorError2 = undefined;

  try {
    for (var _iterator2 = partsArray[Symbol.iterator](), _step2; !(_iteratorNormalCompletion2 = (_step2 = _iterator2.next()).done); _iteratorNormalCompletion2 = true) {
      var part = _step2.value;

      switch (part.type) {
        case "month":
        case "day":
        case "year":
        case "hour":
        case "minute":
        case "second":
          parts[part.type] = Number(part.value);
          break;
      }
    }
  } catch (err) {
    _didIteratorError2 = true;
    _iteratorError2 = err;
  } finally {
    try {
      if (!_iteratorNormalCompletion2 && _iterator2.return != null) {
        _iterator2.return();
      }
    } finally {
      if (_didIteratorError2) {
        throw _iteratorError2;
      }
    }
  }

  return parts;
};

var getDatePartsUsingMoment = function getDatePartsUsingMoment(date, timezone) {
  var momentTZ =  false && false;

  var dateToManipulateTemp = momentTZ.tz(date, timezone);
  return {
    day: dateToManipulateTemp.date(),
    month: dateToManipulateTemp.month() + 1,
    year: dateToManipulateTemp.year(),
    hour: dateToManipulateTemp.hour(),
    minute: dateToManipulateTemp.minute(),
    second: dateToManipulateTemp.second()
  };
};

var temp_getDatePartWithoutIntelSupport = function temp_getDatePartWithoutIntelSupport(dateToManipulateTemp, _timezone) {
  return {
    day: dateToManipulateTemp.getDate(),
    month: dateToManipulateTemp.getMonth() + 1,
    year: dateToManipulateTemp.getFullYear(),
    hour: dateToManipulateTemp.getHours(),
    minute: dateToManipulateTemp.getMinutes(),
    second: 0
  };
};

exports.googleScript = function (name) {
  return "if (window.innerWidth >= '768') { googletag.cmd.push(function() { googletag.display(\"" + name + "\"); }); }";
};

exports.timeAgo = function (date, lang) {
  var startTime = date;
  var endTime = new Date();
  var difference = endTime.getTime() - startTime.getTime(); // This will give difference in milliseconds

  var resultInMinutes = Math.floor(difference / 60000);
  var finalResult = resultInMinutes < 2 ? "עכשיו" : timeago_js_1.format(date, lang);
  return finalResult;
};

exports.channelIconUrl = function (data, getPageCategory) {
  var icon = null;

  if (data.channelIconDisplay === "manual") {
    icon = data.channelIconUrl;
  } else if (data.channelIconDisplay === "category") {
    var category;

    if (data.rightTitleLink) {
      category = getPageCategory(data.rightTitleLink.link.id);
    } else {
      category = getPageCategory(data.tabLink.link.id);
    }

    icon = category && category.icon ? category.icon : null;
  }

  return icon;
};

exports.replaceCharacters = function (str) {
  var obj = {
    '&quot;': "\"",
    '&lt;': "<",
    '&gt;': ">",
    '&amp;': "&",
    '<\/?span[^>]*>': "",
    '&#39;': "'"
  };

  for (var x in obj) {
    str = str.replace(new RegExp(x, 'g'), obj[x]);
  }

  return str;
};


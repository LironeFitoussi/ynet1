var YITSiteWidgets = function YITSiteWidgets() {
  var _this = this;

  _classCallCheck(this, YITSiteWidgets);

  this.rendersIds = null;

  this.renderCallbackCreator = function (parentId) {
    if (_this.rendersIds && _typeof(window['YITSiteWidgetsCallbacks']) == "object" && Array.isArray(window['YITSiteWidgetsCallbacks'])) {
      return function () {
        if (_this.rendersIds.has(parentId)) {
          _this.rendersIds.delete(parentId);

          if (_this.rendersIds.size === 0) {
            console.info("renderFinnished");
            window['YITSiteWidgetsCallbacks'].forEach(function (f) {
              return typeof f == "function" && f();
            });
            window['YITSiteWidgetsCallbacks'] = new YITSiteWidgetsCallbacks();
          }
        }
      };
    }

    return undefined;
  };

  this.render = function (parentId, siteScriptName, props) {
    if (props["__objectDataTypeName"]) {
      console.error("props of object %s with id %s are not a simple object", siteScriptName, parentId);
      return;
    }

    var siteScript = list[siteScriptName];

    if (typeof siteScript == "function") {
      try {
        ReactDOM.render(siteScript(props), document.getElementById(parentId), _this.renderCallbackCreator(parentId));
      } catch (ex) {
        console.error("Error rendering widget ".concat(parentId, "-").concat(siteScriptName), ex);
      }
    } else {
      console.error("Cannot find component ", siteScriptName);
    }
  };

  this.push = function (parms) {
    _this.render.apply(_this, _toConsumableArray(parms));
  };

  if (_typeof(window['YITSiteWidgets']) == "object" && Array.isArray(window['YITSiteWidgets'])) {
    this.rendersIds = new Set(window['YITSiteWidgets'].map(function (parms) {
      return parms[0];
    }));
    window['YITSiteWidgets'].forEach(function (parms) {
      _this.render.apply(_this, _toConsumableArray(parms));
    });
  }

  formatDateDisplay_1.insertDateInSiteServerRender();
};

var YITSiteWidgetsCallbacks = function YITSiteWidgetsCallbacks() {
  _classCallCheck(this, YITSiteWidgetsCallbacks);

  this.push = function (func) {
    return func();
  };
};

var list = (_list = {}, _defineProperty(_list, siteAnalyticsInjector_1.SiteAnalyticsInjector.siteScriptName, function (props) {
  return React.createElement(siteAnalyticsInjector_1.SiteAnalyticsInjector, Object.assign({}, props));
}), _defineProperty(_list, SiteArticleImagesGallery_1.SiteArticleImagesGallery.siteScriptName, function (props) {
  return React.createElement(SiteArticleImagesGallery_1.SiteArticleImagesGallery, Object.assign({}, props));

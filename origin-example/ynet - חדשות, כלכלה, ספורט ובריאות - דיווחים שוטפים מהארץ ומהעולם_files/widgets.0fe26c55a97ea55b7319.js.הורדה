var getAdTagTemplate = function getAdTagTemplate(iu, description_url, cust_params, midroll_params) {
  return "https://pubads.g.doubleclick.net/gampad/ads?sz=640x480|640x360&iu=".concat(iu, "&impl=s&gdfp_req=1&env=vp&output=vast&unviewed_position_start=1&url=[referrer_url]&description_url=").concat(encodeURIComponent(description_url), "&correlator=[timestamp]&cust_params=").concat(encodeURIComponent(cust_params), "&").concat(midroll_params);
};

exports.getCustomParmsString = function (adType, dcPath, yncd, viewsCount, isAutoPlay, isATF, player) {
  var viewStr = viewsCount ? "&view=".concat(viewsCount) : "";
  var playerStr = player ? "&player=".concat(player) : "";
  var permutiv = localStorage.getItem('_psegs');
  var searchUrlParams = new URLSearchParams(window.location.search);
  var value = "VideoPosition=".concat(adType, "&dcPath=").concat(dcPath, "&yncd=").concat(yncd, "&autoplay=").concat(!!isAutoPlay, "&VideoPosition_autoplay=").concat(adType, "_").concat(!!isAutoPlay).concat(viewStr, "&ynch=").concat(dcPath, "&user_intent=").concat(isATF, "&PositionInPage_Autoplay=").concat(isATF ? 'ATF' : 'BTF', "_").concat(!!isAutoPlay).concat(playerStr);

  if (permutiv) {
    value += "&permutive=".concat(encodeURIComponent(permutiv.replace('[', '').replace(']', '')));
  }

  if (searchUrlParams.has('dcStage')) {
    value += "&dcStage=".concat(searchUrlParams.get('dcStage'));
  }

  if (window.dcSite && window.dcSite.includes("mynet")) {
    var mynet_platform = window.isMobile1 ? "mobile" : "desktop";
    var sitestr = window.dcSite;
    var mynet_cities = sitestr.substr(5, sitestr.length);
    value += "&mynet_platform=".concat(mynet_platform, "&mynet_cities=").concat(mynet_cities);
  }

  if (false) {}

  if (false) {}

  return value;
};

function getImaSettings(isAutoPlay, isATF) {
  var player = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : "";
  var duration = arguments.length > 3 ? arguments[3] : undefined;
  var ads = [{
    time: 0,
    adTag: buildAdTag("Preroll", isAutoPlay, isATF, player, duration)
  }];

  if (window && window['videoPlayerAds']) {
    ads = window['videoPlayerAds'];
  }

  return {
    locale: "he",
    ads: ads
  };
}

exports.getImaSettings = getImaSettings;
var midrollIntervalInSeconds = 8 * 60;
var midrollLimitFromEndInSeconds = 2 * 60;

function addMidrollAds(api, isAutoPlay, isATF) {
  var player = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : "";
  //use one to only fire the event once so that multiple plays of the same video will not ad unnecessary ads. 
  api.one("ready", function () {
    var mid = midrollIntervalInSeconds;
    var maxDuration = api.video.duration - midrollLimitFromEndInSeconds;

    while (mid < maxDuration) {
      api.conf.ima && api.conf.ima.ads && api.conf.ima.ads.push({
        time: mid,
        adTag: buildAdTag("Midroll", isAutoPlay, isATF, player)
      });
      mid += midrollIntervalInSeconds;
    }
  });
}

exports.addMidrollAds = addMidrollAds;

function buildAdTag(adType, isAutoPlay, isATF, player, duration) {
  var viewsCount = getNumberOfViews();
  var customParms = exports.getCustomParmsString(adType, window.dcPath, window.video_ad_yncd, viewsCount, isAutoPlay, isATF, player);
  var videoAdIu = getPlayerAdIu();
  var midRollParms = exports.getMidrollParams(duration);
  return getAdTagTemplate(videoAdIu, window.video_ad_description_url, customParms, midRollParms);
}

exports.buildAdTag = buildAdTag;

function getPlayerAdIu() {
  var videoAdIu = window.video_ad_iu;

  if (window.isMobile1) {
    videoAdIu = window.video_ad_iu_mobile;

    if (videoAdIu) {
      var device = getDevice();
      videoAdIu = videoAdIu.replace("{device}", device); // if there is no '{device}' in videoAdIu, this will do nothing
    }
  }

  return videoAdIu;
}

exports.getPlayerAdIu = getPlayerAdIu;

function getNumberOfViews() {
  try {
    resetLocalStorageVideoViewCounter();
    return ++localStorage._ads_video_view;
  } catch (_) {
    return null;
  }
}

exports.getNumberOfViews = getNumberOfViews;
var threeHoursInMS = 3 * 60 * 60 * 1000;

function resetLocalStorageVideoViewCounter() {
  var dayOfMonthInThreeHours = new Date(Date.now() + threeHoursInMS).getUTCDate();

  if (dayOfMonthInThreeHours != localStorage._ads_video_view_Date) {
    localStorage._ads_video_view_Date = dayOfMonthInThreeHours;
    localStorage._ads_video_view = 0;
  }
}

exports.getMidrollParams = function (duration) {
  var defaultVid_d = 90000;
  var maxDuration = 25000;
  var allcues = "30000,60000";
  var vid_d = duration || defaultVid_d;
  return "max_ad_duration=".concat(maxDuration, "&allcues=").concat(allcues, "&vid_d=").concat(vid_d, "&ad_rule=1");
};

function getDevice() {
  var device = 'android';

  if (typeof navigator !== "undefined") {
    if (/iPhone|iPad|iPod/i.test(navigator.userAgent)) {
      device = 'iphone';
    }
  }

  return device;
}

exports.getDevice = getDevice;


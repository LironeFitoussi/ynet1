/***/ "../wcm_front-js/app/widgets/defenitions/blogsAutoFeed/blogsAutoFeedUtils.js":
/***/ (function(module, exports, __webpack_require__) {

"use strict";


Object.defineProperty(exports, "__esModule", {
  value: true
});

exports.getHourAndMinutes = function (date) {
  return "".concat(date.getHours(), ":").concat((date.getMinutes() < 10 ? '0' : '') + date.getMinutes());
};

exports.getFullDate = function (date) {
  var devideUnitChar = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : ".";
  var format = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : "";

  switch (format) {
    case "shortYear":
      return "".concat(date.getDate()).concat(devideUnitChar).concat(date.getMonth() + 1).concat(devideUnitChar).concat("".concat(date.getFullYear()).slice(2));
      break;

    default:
      return "".concat(date.getDate()).concat(devideUnitChar).concat(date.getMonth() + 1).concat(devideUnitChar).concat(date.getFullYear());
  }
};

exports.isSameDate = function (date1, date2) {
  return exports.getFullDate(date1).localeCompare(exports.getFullDate(date2)) === 0;
};

exports.scrollToElement = function (id) {
  var block = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : "start";
  var inline = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : "nearest";
  var el = document.querySelector(".".concat(id));
  el && el.scrollIntoView({
    behavior: "smooth",
    block: block,
    inline: inline
  });
};

exports.isToday = function (date) {
  var today = new Date();
  return date.getDate() == today.getDate() && date.getMonth() == today.getMonth() && date.getFullYear() == today.getFullYear();
};

exports.isYesterday = function (date) {
  var yesterday = new Date(new Date().valueOf() - 1000 * 60 * 60 * 24); // love was such an easy game to play

  return date.getDate() == yesterday.getDate() && date.getMonth() == yesterday.getMonth() && date.getFullYear() == yesterday.getFullYear();
};

exports.isXMinutesOld = function (date, minutes) {
  if (!exports.isToday(date)) return true;
  var today = new Date();
  var diffMs = date.getTime() - today.getTime(); // milliseconds between now & date  

  var diffMins = Math.abs(Math.round(diffMs % 86400000 % 3600000 / 60000)); // minutes

  return diffMins >= minutes;
};


function getLinkResource(link) {
  switch (link.type) {
    case contentTypes_1.ContentTypes.ARTICLE:
    case contentTypes_1.ContentTypes.TAG:
    case contentTypes_1.ContentTypes.CATEGORY:
    case contentTypes_1.ContentTypes.FILE:
      return {
        resourceId: link.link.id,
        resourceType: link.type
      };

    default:
      return null;
  }
}

exports.getLinkResource = getLinkResource;

function getLinkResourceWithVideo(link) {
  switch (link.type) {
    case contentTypes_1.ContentTypes.MEDIA_VIDEO:
      return {
        resourceId: link.link.id,
        resourceType: link.type
      };

    default:
      return getLinkResource(link);
  }
}


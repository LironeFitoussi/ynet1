      linkTarget = item.linkTarget;

  var getDiffDateInString = function getDiffDateInString() {
    var diffInDays = formatDateDisplay_1.getDiffFromToday(startProgram);
    var day = String(new Date(startProgram).getDate()).padStart(2, '0');
    var month = String(new Date(startProgram).getMonth() + 1).padStart(2, '0');
    var year = new Date(startProgram).getFullYear().toString().substr(-2);
    var date = "".concat(day, ".").concat(month, ".").concat(year);

    if (diffInDays === 1) {
      return "אתמול"; // '2 days ago' etc.
    }

    if (diffInDays === -1) {
      return "מחר";
    }

    if (diffInDays > 1 || diffInDays < -1) {
      return date;
    }

    return "";
  };

  var diff = getDiffDateInString();
  var broadcastHours = hideHours ? "".concat(diff ? diff : "") : "".concat(diff ? diff + " |" : "", " ").concat(startTime, " - ").concat(endTime);
  return React.createElement("div", {
    className: "schedule-list-item ".concat(isProgramFinished ? "passed" : ""),

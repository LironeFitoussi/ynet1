        className: "loadMore"
      }, React.createElement("button", {
        className: "loadMoreButton",
        disabled: !isLoadMore || articles.length === 0,
        onClick: this.loadMoreArticles
      }, isLTR ? 'Load more' : 'טען עוד'))));
    }
  }]);

  return SiteCtechTwentyFourSevenWideComponenta;
}(React.Component);

SiteCtechTwentyFourSevenWideComponenta.siteScriptName = "SiteCtechTwentyFourSevenWideComponenta";
exports.SiteCtechTwentyFourSevenWideComponenta = SiteCtechTwentyFourSevenWideComponenta;

function toDateWithoutHour(date) {
  return "".concat(formatNum(date.getDate()), ".").concat(formatNum(date.getMonth() + 1), ".").concat(date.getFullYear().toString().substr(-2));
}

exports.toDateWithoutHour = toDateWithoutHour;

function toHourWithoutDate(date) {
  var minutes = formatNum(date.getMinutes());
  var hours = formatNum(date.getHours());
  return "".concat(hours, ":").concat(minutes);
}

exports.toHourWithoutDate = toHourWithoutDate;

function datesAreOnSameDay(first, second) {
  return first.getFullYear() === second.getFullYear() && first.getMonth() === second.getMonth() && first.getDate() === second.getDate();
}

exports.datesAreOnSameDay = datesAreOnSameDay;

function formatNum(num) {
  var displayNum = num < 10 ? '0' + num : num;
  return displayNum;
}

function datesAreOnSameWeek(first, second, endOfTheWeek) {
  var oneDay = 1000 * 60 * 60 * 24;
  var diffInTime = second.getTime() - first.getTime();
  var diffInDays = Math.round(diffInTime / oneDay);
  return diffInDays >= -endOfTheWeek;
}

exports.datesAreOnSameWeek = datesAreOnSameWeek;
/* WEBPACK VAR INJECTION */}.call(this, __webpack_require__("./node_modules/react/index.js")))


exports.getImgTitle = function (image) {
  var title;

  if (image) {
    var shouldAddBrekets = image.credit && image.credit.trim() && image.caption && image.caption.trim();
    title = "".concat(image.caption || "").concat(shouldAddBrekets && " (").concat(image.credit || "").concat(shouldAddBrekets && ") ");
  } else {
    title = "";
  }

  return title;
};

exports.getImgAlt = function (image) {
  var alt;

  if (image) {
    if (image.alt && image.alt.trim() !== "") {
      alt = image.alt;
    } else {
      alt = image.caption;
    }
  } else {
    alt = "";
  }

  return alt;
};

exports.isAriaHidden = function (image) {
  var isAriaHidden;

  if (image && image.publishedLink) {
    isAriaHidden = false;
  } else {
    isAriaHidden = true;
  }

  return isAriaHidden;
};

exports.siteMediaConverter = function (imageSize, mediaData, getImage, getVideo, videoBehavior, itemTitle, itemLinkUrl, isBigVideo, isMutam) {
  var medias = map(mediaData, function (m, i) {
    if (m.mediaType === contentTypes_1.MediaContentTypes.MEDIA_IMAGE) {
      var image = (isMutam && m.mobileImageId ? getImage(m.mobileImageId) : getImage(m.resourceId)) || {};
      var siteImage = {
        mediaType: contentTypes_1.MediaContentTypes.MEDIA_IMAGE,
        url: ApiUrls.getImageUrl(image, imageSize),
        alt: exports.getImgAlt(image),
        credit: image.credit,
        ariaHidden: exports.isAriaHidden(image),
        title: exports.getImgTitle(image),
        className: "gallery".concat(i, " ")
      };
      return siteImage;
    } else {
      var video = getVideo(m.resourceId);
      var siteVideo = Object.assign({}, videoBehavior, {
        mediaType: contentTypes_1.MediaContentTypes.MEDIA_VIDEO,
        url: video.main_link,
        downGradeUrl: video.low_res_link,
        poster: video.posterSrc,
        title: itemTitle,
        itemLinkUrl: itemLinkUrl,
        isBigVideo: isBigVideo,
        youtube_id: video.youtube_id,
        mediaId: video.mediaVideoId,
        blockAbroadViews: video.blockAbroadViews
      });
      return siteVideo;
    }
  });
  return medias;
};

exports.hexToRgbA = function (hex, opacity) {
  if (hex.startsWith("##")) {
    hex = hex.replace(/##/g, "#");
  }

  var defaultColor = "1, 1, 1",
      defaultOpacity = "1";
  var color;

  try {
    if (/^#([A-Fa-f0-9]{3}){1,2}$/.test(hex)) {
      color = hex.substring(1).split('');

      if (color.length == 3) {
        color = [color[0], color[0], color[1], color[1], color[2], color[2]];
      }

      color = "0x".concat(color.join(''), " ");
      return "rgba(".concat([color >> 16 & 255, color >> 8 & 255, color & 255].join(','), ", ").concat(opacity || defaultOpacity, ") ");
    }
  } catch (e) {
    return "rgba(".concat(defaultColor, ", ").concat(defaultOpacity, ") ");
  }

  return "rgba(".concat(defaultColor, ", ").concat(defaultOpacity, ") ");
};

exports.formatProperArticleDateForRender = function (launchDate, dateUpdatedOnSite) {
  if (dateUpdatedOnSite) {
    var dateUpdatedOnSiteDateObj = typeof dateUpdatedOnSite === "string" ? new Date(dateUpdatedOnSite) : dateUpdatedOnSite;
    return exports.separateHourFromDateAndFormatToString(dateUpdatedOnSiteDateObj);
  }

  if (launchDate) {
    var launchDateDateObj = typeof launchDate === "string" ? new Date(launchDate) : launchDate;
    return exports.separateHourFromDateAndFormatToString(launchDateDateObj);
  }

  return "";
};

exports.concatStringsWithCommaSeperator = function (strings) {
  var string = "";
  var _iteratorNormalCompletion = true;
  var _didIteratorError = false;
  var _iteratorError = undefined;

  try {
    for (var _iterator = strings[Symbol.iterator](), _step; !(_iteratorNormalCompletion = (_step = _iterator.next()).done); _iteratorNormalCompletion = true) {
      var s = _step.value;
      string = string.concat("".concat(s, ", "));
    }
  } catch (err) {
    _didIteratorError = true;
    _iteratorError = err;
  } finally {
    try {
      if (!_iteratorNormalCompletion && _iterator.return != null) {
        _iterator.return();
      }
    } finally {
      if (_didIteratorError) {
        throw _iteratorError;
      }
    }
  }

  return string.replace(/,\s*$/, "");
};

var defaultDateFormatOptions = {
  showDate: true,
  showHour: true,
  dateTimeSeperator: ", ",
  padDateWithZero: true,
  padHourWithZero: true,
  fullYear: false,
  isSlashSeparator: false,
  isDateFirst: false,
  datePartOrder: interfaces_1.DateFormat.dmy
};

exports.separateHourFromDateAndFormatToString = function (dateToManipulate, options) {
  var dateToManipulateTemp = typeof dateToManipulate === "string" ? new Date(dateToManipulate) : dateToManipulate;

  if (!dateToManipulateTemp || !isFinite(dateToManipulateTemp) || !dateToManipulateTemp.getDate) {
    return "";
  }

  options = Object.assign({}, defaultDateFormatOptionsBySite_1.getDefaultDateFormatBySite(serverConfig_1.getLocaleDateFormat()), options);
  var _options = options,
      showDate = _options.showDate,
      showHour = _options.showHour,
      dateTimeSeperator = _options.dateTimeSeperator,
      padDateWithZero = _options.padDateWithZero,
      padHourWithZero = _options.padHourWithZero,
      fullYear = _options.fullYear,
      isSlashSeparator = _options.isSlashSeparator,
      isDateFirst = _options.isDateFirst,
      datePartOrder = _options.datePartOrder;
  var dateParts = getDateParts(dateToManipulateTemp, serverConfig_1.getTimezone());
  var dd = dateParts.day;
  var mm = dateParts.month;
  var yy = dateParts.year.toString().substr(-2);
  var yyyy = dateParts.year;
  var hh = dateParts.hour == 24 ? 0 : dateParts.hour;
  var MM = dateParts.minute;
  var yearFormat = fullYear ? yyyy : yy;
  var datePartFormat = padDateWithZero ? "%02d" : "%d";
  var hourPartFormat = padHourWithZero ? "%02d" : "%d";
  var separatorFormat = isSlashSeparator ? "/" : ".";
  var date = datePartOrder === interfaces_1.DateFormat.mdy ? sprintf_js_1.sprintf("".concat(datePartFormat).concat(separatorFormat).concat(datePartFormat).concat(separatorFormat, "%d"), mm, dd, yearFormat) : sprintf_js_1.sprintf("".concat(datePartFormat).concat(separatorFormat).concat(datePartFormat).concat(separatorFormat, "%d"), dd, mm, yearFormat);
  var time = sprintf_js_1.sprintf("".concat(hourPartFormat, ":%02d"), hh, MM);
  if (showDate && showHour) return isDateFirst ? "".concat(date).concat(dateTimeSeperator).concat(time, " ") : "".concat(time).concat(dateTimeSeperator).concat(date, " ");
  if (showDate) return date;
  if (showHour) return time;
  return "";
};

exports.calculateShouldAddArticleAutoplayIndicator = function (icon, media, isIconVisible) {
  var isImageMedia;
  if (isIconVisible) return icon === "video" || icon === "automatic";else {
    if (media.mediaType) {
      isImageMedia = media.mediaType === contentTypes_1.MediaContentTypes.MEDIA_IMAGE;
    } else {
      var m = media;
      isImageMedia = m.array.length <= 1 && m.get(0).mediaType === contentTypes_1.MediaContentTypes.MEDIA_IMAGE;
    }

    return isImageMedia && (icon === "video" || icon === "automatic");
  }
};

exports.AUTOPLAY_HASH_TAG = "#autoplay";

exports.calculateAutomaticComponentaUrl = function (item) {
  return item.publishedLink + (item.hasVideo ? exports.AUTOPLAY_HASH_TAG : "");
};

exports.getQueryStringValue = function (key) {
  return decodeURIComponent(window.location.search.replace(new RegExp("^(?:.*[&\\?]" + encodeURIComponent(key).replace(/[\.\+\*]/g, "\\$&") + "(?:\\=([^&]*))?)?.*$", "i"), "$1"));
};

exports.isQaHost = function (hostUrl) {
  return hostUrl.toLocaleLowerCase().includes("qa");
};

exports.openFacebookShare = function (url) {
  var textToShare = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : "";
  var analyticsCb = arguments.length > 2 ? arguments[2] : undefined;
  var content = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : "";
  var current_article_id = arguments.length > 4 ? arguments[4] : undefined;
  var hashPart = arguments.length > 5 ? arguments[5] : undefined;
  var widthAndHeight = "width=500, height=500";
  var hostname = window.location.hostname.replace("www.", "");
  var qText = textToShare.trim() ? "&qtext=".concat(textToShare) : "";
  var utmContent = content.trim() ? "&utm_content=".concat(content) : "";
  url = "".concat(url, "?utm_source=").concat(hostname, "&utm_medium=Share&utm_campaign=Facebook&utm_term=").concat(current_article_id ? current_article_id : window.articleId).concat(utmContent).concat(qText);
  url = hashPart ? url + "#" + hashPart : url;
  window.open("//www.facebook.com/sharer/sharer.php?u=" + encodeURIComponent(url), "", widthAndHeight);
  analyticsCb && typeof analyticsCb === "function" && analyticsCb();
};

exports.openLinkedinShare = function (url, analyticsCb, hashPart) {
  var widthAndHeight = "width=500, height=500";
  var hostname = window.location.hostname.replace("www.", ""); // const qText = textToShare.trim() ? `&qtext=${textToShare}` : "";
  //  const utmContent = content.trim() ? `&utm_content=${content}` : "";

  url = hashPart ? url + "#" + hashPart : url;
  window.open("//linkedin.com/sharing/share-offsite/?url=" + encodeURIComponent(url), "", widthAndHeight);
  analyticsCb && typeof analyticsCb === "function" && analyticsCb();
};

exports.openMailShare = function (body, siteName, url, analyticsCb) {
  var content = arguments.length > 4 && arguments[4] !== undefined ? arguments[4] : "";
  var hashPart = arguments.length > 5 ? arguments[5] : undefined;
  var convertedBody = exports.replaceCharacters(body);
  var title = "כתבה מעניינת מ";
  var hostname = window.location.hostname.replace("www.", "");
  var utmContent = content.trim() ? "&utm_content=".concat(encodeURIComponent(content)) : "";
  url = "".concat(url, "?utm_source=").concat(encodeURIComponent(hostname), "&utm_medium=Share&utm_campaign=Mail").concat(utmContent);
  url = hashPart ? url + "#" + hashPart : url;
  window.location.href = "mailto:?subject=".concat(title).concat(siteName, "&body=").concat(encodeURIComponent(convertedBody), " %0d %0d ").concat(encodeURIComponent(url));
  analyticsCb && typeof analyticsCb === "function" && analyticsCb();
};

exports.openTwitterShare = function (url, analyticsCb) {
  var content = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : "";
  var current_article_id = arguments.length > 3 ? arguments[3] : undefined;
  var hashPart = arguments.length > 4 ? arguments[4] : undefined;

function addVideoInfoToFixList(playerContainer, data) {
  var titleInnerHtml = getYnetTvVideoText(data.title, "vp-title");
  var subTitleInnerHtml = getYnetTvVideoText(data.subTitle, "vp-subTitle");
  var linkTextInnerHtml = getVideoArticleLink(data.linkText, data.itemLinkUrl);
  var playList = playerContainer.querySelector(".fp-playlist");
  var infoElement = document.createElement("div");
  infoElement.className = "vp-info";
  infoElement.innerHTML = titleInnerHtml + subTitleInnerHtml + linkTextInnerHtml;
  var oldInfo = playerContainer.querySelector(".vp-info");

  if (oldInfo) {
    oldInfo.remove();
    playList.appendChild(infoElement);
  } else playList.appendChild(infoElement);
}


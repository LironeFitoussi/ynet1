      var newFiles = reject(selectedFiles, function (f, i) {
        return i === index;
      });

      _this.setState({
        selectedFiles: newFiles
      });

      _this.handleNumOfFiles("remove");
    };

    _this.sendFiles = function (files) {
      var numOfFiles = _this.state.numOfFiles;

      for (var i = 0; i < files.length; i++) {
        var file = files[i];

        if (_this.containsHeb(file.name)) {
          var ext = file.name.split('.').pop();
          file = _this.renameFile(file, "custom_file_".concat(numOfFiles + 1, ".").concat(ext));
        }

        var existingFiles = _this.state.selectedFiles;
        var key = Date.now();
        existingFiles[key] = {
          fileName: file.name
        };
        file["key"] = key;

        _this.setState({
          selectedFiles: Object.assign({}, existingFiles)
        });

        _this.manageFileUpload(_this.state.s3Policy, file);
      }
    };

    _this.containsHeb = function (str) {
      return /[\u0590-\u05FF]/.test(str);
    };

    _this.renameFile = function (originalFile, newName) {
      return new File([originalFile], newName, {
        type: originalFile.type,
        lastModified: originalFile.lastModified
      });
    };

    _this.isUploadFinished = function () {
      var isUploadFinished;
      Object.keys(_this.state.selectedFiles).forEach(function (fileKey) {
        isUploadFinished = !has(_this.state.selectedFiles[fileKey], "progress");
      });

      if (isUploadFinished) {
        _this.setSubmitButtonEnabled;
      } else {
        _this.setSubmitButtonDisabled;
      }
    };

    _this.isFilesSizeValid = function (files) {
      var invalidFilesName = _this.state.invalidFilesName;
      var dt = new DataTransfer();

      for (var i = 0; i < files.length; i++) {
        var file = files[i];

        if (file.size > MAX_SIZE_ALLOWED_PER_FILE || file.size < 1) {
          _this.setState({
            invalidFilesName: [].concat(_toConsumableArray(invalidFilesName), [file.name])
          });

          continue;
        }

        dt.items.add(file);
      }

      return dt.files;
    };


exports.linkedSlotUpdateAfterArticleRecourceLoaded = function (data, _ref, _ref2, _ref3) {
  var copyImage = _ref.copyImage;
  var articleData = _ref2.articleData,
      image = _ref2.image;
  var setSubtitle = _ref3.setSubtitle,
      setTitle = _ref3.setTitle,
      setAuthor = _ref3.setAuthor,
      insertRoofToTitle = _ref3.insertRoofToTitle,
      shouldCopyToMobile = _ref3.shouldCopyToMobile,
      keepMedia = _ref3.keepMedia;

  if (insertRoofToTitle === undefined) {
    insertRoofToTitle = true;
  }


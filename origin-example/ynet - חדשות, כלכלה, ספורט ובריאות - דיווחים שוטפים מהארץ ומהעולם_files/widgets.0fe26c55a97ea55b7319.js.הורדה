exports.dateParser = function (key, value) {
  if (typeof value === 'string' && ISO_8601.test(value)) {
    var newValue;

    if (!value.endsWith("Z")) {
      newValue = "".concat(value, "Z");
      console.error("WARNING date without Z ending", value);
    } else newValue = value;

    return new Date(newValue);
  }

  return value;

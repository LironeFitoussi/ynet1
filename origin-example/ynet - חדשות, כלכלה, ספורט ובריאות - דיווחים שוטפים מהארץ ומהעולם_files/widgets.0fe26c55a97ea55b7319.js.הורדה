var replaceCharacters = function replaceCharacters(str) {
  var obj = {
    '&quot;': "\"",
    '&lt;': "<",
    '&gt;': ">",
    '&amp;': "&",
    '<\/?span[^>]*>': "",
    '&#39;': "'"
  };

  for (var x in obj) {
    str = str.replace(new RegExp(x, 'g'), obj[x]);
  }

  return str;
};

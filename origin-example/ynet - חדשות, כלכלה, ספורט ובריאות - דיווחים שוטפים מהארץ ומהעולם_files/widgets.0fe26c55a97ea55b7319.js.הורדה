var RadioScheduleListItem = function RadioScheduleListItem(props) {
  var item = props.item,
      isCurrentShow = props.isCurrentShow,
      isProgramFinished = props.isProgramFinished,
      index = props.index;
  var startTime = item.startTime,
      endTime = item.endTime,
      title = item.title,
      startProgram = item.startProgram,
      hideHours = item.hideHours,

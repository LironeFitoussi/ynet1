exports.getItemMargin = function (componentWidth, imageWidth) {
  var singleItemWidth = imageWidth;
  var numberofVisibleItems = Math.floor(componentWidth / singleItemWidth);
  var imagesWidth = numberofVisibleItems * singleItemWidth;
  var spaceForMargin = componentWidth - imagesWidth;
  var marginLeft = spaceForMargin / (numberofVisibleItems - 1);
  return marginLeft;
};


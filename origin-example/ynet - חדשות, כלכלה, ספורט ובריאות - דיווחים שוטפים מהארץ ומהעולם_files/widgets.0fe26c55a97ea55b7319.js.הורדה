function getVideoArticleLink(linkText, itemLinkUrl) {
  if (linkText) {
    return "<a href=\"".concat(itemLinkUrl, "\" target=\"_blank\" >").concat(linkText, "<img class=\"campaing_title\" aria-hidden=\"true\" src=\"").concat(getYnetTvArrow(), "\" /></a>");
  }

  return "";
}

exports.getVideoArticleLink = getVideoArticleLink;

function getYnetTvVideoText(text, className) {
  if (text) {
    return "<p class=\"".concat(className, "\">").concat(text, "</p>");
  }

  return "";
}

exports.getYnetTvVideoText = getYnetTvVideoText;

function addYnetTvLogoToHeader(playerContainer) {
  var header = playerContainer.querySelector(".fp-header");
  var ynetTvLogo = document.createElement("img");
  ynetTvLogo.className = "ynetTvLogo";
  ynetTvLogo.src = getYnetTvLogoUrl();
  header.appendChild(ynetTvLogo);
}


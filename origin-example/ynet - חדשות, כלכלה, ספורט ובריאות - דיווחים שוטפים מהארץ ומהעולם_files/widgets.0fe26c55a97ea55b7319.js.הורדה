exports.isYouTubeGlobalSettingEnabled = function (data) {
  return typeof window != "undefined" && window.youtube_player == "1" && data && !!data.youtube_id;
};

function getVideoSources(mainUrl, downGradeUrl) {
  var getVideoType = function getVideoType(url) {
    return url.toLowerCase().endsWith(".m3u8") ? "application/x-mpegurl" : "video/mp4";
  };

  var removeProtocolFromUrl = function removeProtocolFromUrl(url) {
    return url.replace(/(^\w+:)/, '');
  };

  var getSource = function getSource(url) {
    return url ? {
      type: getVideoType(url),
      src: removeProtocolFromUrl(url)
    } : undefined;
  };

  var sources = [];
  var mainSource = getSource(mainUrl);
  var downGradeSource = getSource(downGradeUrl);
  if (mainSource) sources.push(mainSource);
  if (downGradeSource) sources.push(downGradeSource);
  return sources;
}

exports.getVideoSources = getVideoSources;

function getLogoUrl() {
  var siteName = window && window["siteID"];

  switch (siteName) {
    case "ynet":
      return "//mediadownload.ynet.co.il/flowplayerlive/ynetlogo.png";

    case "vesty":
      return "https://www.vesty.co.il/images/ynetlvestylogo.png";

    case "ynetnews":
      return "https://ynet-images1.yit.co.il/picserver5/wcm_upload/2019/03/28/SJ8Xwm9dN/CENTRAL_1024_ynet_logo_en.png";

    default:
      return "";
  }
}

exports.getLogoUrl = getLogoUrl;

function addTitleToHeader(playerContainer, title, itemLinkUrl) {
  var titleInnerHtml = getVideoTitle(title, itemLinkUrl);
  if (!titleInnerHtml) return;
  var header = playerContainer.querySelector(".fp-header");
  var titleElement = document.createElement("div");
  titleElement.className = "vp-title";
  titleElement.innerHTML = titleInnerHtml;
  header.appendChild(titleElement);
}

exports.addTitleToHeader = addTitleToHeader;


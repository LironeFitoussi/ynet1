      var resultId = _.values(result)[0].toString();

      return {
        type: matchFunction.type,
        id: resultId
      };
    }
  }
};

exports.convertHtmlToString = function (text) {
  var ltrDiv = '<div style="direction: ltr;"></div>';
  var ltrDivRegex = new RegExp(ltrDiv, 'g');
  return text ? text.replace(/&nbsp;/g, " ").replace(/&amp;/g, "&").replace(ltrDivRegex, '') : "";
};

exports.convertLanguageCodeToISO = function (languageCode) {
  return languageCode && languageCode.slice(0, 2) || "en";
};


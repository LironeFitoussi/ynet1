function addStartEvent(api, videoId, wcmPopularityLink) {
  api.on("cuepoint", function (e, api, cuepoint) {
    if (cuepoint.startEvent) {
      // window.dataLayer = window.dataLayer || []
      // window.dataLayer.push({
      //     event: "video_start",
      //     related_url: `${window.video_ad_description_url}&dcPath=${window.dcPath}`,
      //     video_url: `${api.video.src}&source=${window.video_analytics_src}`
      // });
      sendPopularityEvent(videoId, api.video.duration, wcmPopularityLink);
    }
  });
}

function sendPopularityEvent(videoId, videoTime, wcmPopularityLink) {
  var typeNum = "1";
  typeNum = window.isMobile1 ? "2" : "1";

  if (wcmPopularityLink) {
    var url = "".concat(wcmPopularityLink, "?content=").concat(videoId, "&type=").concat(typeNum, "&time=").concat(videoTime, "&random=").concat(Math.floor(Math.random() * 999999));
    new Image().src = url;
  }
}

exports.sendPopularityEvent = sendPopularityEvent;


/***/ "../wcm_front-js/app/widgets/siteWidgets/formatDateDisplay.js":
/***/ (function(module, exports, __webpack_require__) {

"use strict";


Object.defineProperty(exports, "__esModule", {
  value: true
});

exports.formatDateDisplay = function (date, isArticleStr, isMinutesAgo) {
  if (date) {
    var isArticle = isArticleStr === "1";
    var dateToManipulateTemp = new Date(date);
    var diffFromToday = getDiffFromToday(dateToManipulateTemp);

    switch (diffFromToday) {
      case 0:
        return getToday(dateToManipulateTemp, isMinutesAgo);

      case 1:
        return getYesterday(dateToManipulateTemp);

      default:
        return getRegularDate(dateToManipulateTemp, isArticle);
    }
  } else return "";
};

function formatNum(num) {
  var displayNum = num < 10 ? '0' + num : num;
  return displayNum;
}

var hours = function hours(date) {
  return formatNum(date.getHours());
};

var minutes = function minutes(date) {
  return formatNum(date.getMinutes());
};

var days = function days(date) {
  return formatNum(date.getDate());
};

var month = function month(date) {
  return formatNum(date.getMonth() + 1);
};

var year = function year(date) {
  return date.getFullYear().toString().slice(-2);
};

function getToday(date, isMinutesAgo) {
  var time = "".concat(hours(date), ":").concat(minutes(date));

  if (isMinutesAgo && minutesDiff(date) < 60) {
    if (window && window.languageCode) {
      if (window.languageCode.slice(0, 2) === "en") {
        time = "".concat(minutesDiff(date), " minutes ago");
      } else if (window.languageCode === "ru-RU") {
        time = "".concat(minutesDiff(date), " \u043C\u0438\u043D\u0443\u0442 \u043D\u0430\u0437\u0430\u0434");
      } else if (window.languageCode === "he-IL") {
        time = "\u05DC\u05E4\u05E0\u05D9 ".concat(minutesDiff(date), " \u05D3\u05E7\u05D5\u05EA");
      }
    }
  }

  return time;
}

function minutesDiff(date) {
  var secDiff = Math.floor((Date.now() - new Date(date).getTime()) / 1000);
  var minutesDiff = Math.floor(secDiff / 60);
  return minutesDiff;
}

function getYesterday(date) {
  var yesterday = "אתמול";

  if (window && window.languageCode) {
    if (window.languageCode.slice(0, 2) === "en") {
      yesterday = "Yesterday";
    } else if (window.languageCode === "ru-RU") {
      yesterday = "Вчера";
    } else if (window.languageCode === "he-IL") {
      yesterday = "אתמול";
    }
  }

  var time = "".concat(yesterday, " | ").concat(hours(date), ":").concat(minutes(date));
  return time;
}

function getRegularDate(date, isArticle) {
  var dateView = "".concat(days(date), ".").concat(month(date), ".").concat(year(date));

  if (window && window.languageCode) {
    if (window.languageCode.slice(0, 2) === "en") {
      dateView = "".concat(month(date), ".").concat(days(date), ".").concat(year(date));
    }
  }

  var time = isArticle ? "".concat(dateView, " | ").concat(hours(date), ":").concat(minutes(date)) : dateView;
  return time;
}

function getDiffFromToday(date) {
  return dateDaysDiff(date, new Date());
}

exports.getDiffFromToday = getDiffFromToday;

function dateDaysDiff(date1, date2) {
  var timeDiff = getDateOnly(date2).getTime() - getDateOnly(date1).getTime();
  var diffDays = Math.ceil(timeDiff / (1000 * 3600 * 24));
  return diffDays;
}

function getDateOnly(date) {
  var d = new Date(date);
  d.setHours(0, 0, 0, 0);
  return d;
}

exports.insertDateInSiteServerRender = function () {
  if (typeof window !== "undefined") {
    var elements = document.querySelectorAll('[data-wcmdate]');
    var _iteratorNormalCompletion = true;

  ImageSize[ImageSize["xxLarge"] = 5] = "xxLarge";
})(ImageSize = exports.ImageSize || (exports.ImageSize = {}));

function imageSizeConvertion(size) {
  if (size.imageSize) return size.imageSize; //if (size.w <= 155) return ImageSize.small

  if (size.w <= 320) return ImageSize.medium;
  if (size.w <= 490) return ImageSize.large;
  if (size.w <= 1024) return ImageSize.xLarge;
  return ImageSize.xxLarge;
}


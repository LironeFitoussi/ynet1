              });

              votePostData.talkback_like = true;
              votePostData.talkback_unlike = true;
            } else {
              _this.setState(function (pState) {
                return {
                  vote: 0,
                  talkback_like: pState.talkback_like + 1
                };
              });
            }

            axios.post(voteApiUrl, votePostData);
            break;
        }
      } else {
        switch (vote) {
          // case -1 = if the user already unliked - set vote to 0 and like the vote
          case -1:
            _this.setState(function (pState) {
              return {
                vote: 0,
                unlikes: pState.unlikes - 1
              };
            });

            votePostData.talkback_like = true;
            votePostData.talkback_unlike = false;
            axios.post(voteApiUrl, votePostData);
            break;

          case 0:
            _this.setState(function (pState) {
              return {
                vote: -1,
                unlikes: pState.unlikes + 1,
                talkback_like: pState.talkback_like - 1
              };
            });

            axios.post(voteApiUrl, votePostData);
            break;

          case 1:
            if (voteType === "3state") {
              _this.setState(function (pState) {
                return {
                  vote: -1,
                  likes: pState.likes - 1,
                  unlikes: pState.unlikes + 1
                };
              });

              votePostData.talkback_like = false;
              votePostData.talkback_unlike = false;
            } else {
              _this.setState(function (pState) {
                return {
                  vote: 0,
                  talkback_like: pState.talkback_like - 1
                };
              });
            }

            axios.post(voteApiUrl, votePostData);
            break;
        }
      }
    };

    var comment = props.comment;
    var vote = 0;

    if (window && comment && comment.id) {
      var voteStr = Cookies.get("talkback_".concat(comment.id));

      switch (voteStr) {
        case "True":
          vote = 1;
          break;

        case "False":
          vote = -1;
          break;

        default:
          vote = 0;
      }
    }

    _this.state = {
      isExpanded: false,
      isAddCommentFormVisible: false,
      commentFormType: null,
      vote: vote,
      talkback_like: comment.talkback_like,
      likes: comment.likes,
      unlikes: comment.unlikes
    };
    return _this;
  }

  _createClass(ArticleCommentWrapper, [{
    key: "componentWillMount",
    value: function componentWillMount() {
      var _this$props3 = this.props,
          isAutoExpanded = _this$props3.isAutoExpanded,
          IdOfLastTalkbackExpandedByUser = _this$props3.IdOfLastTalkbackExpandedByUser,
          id = _this$props3.comment.id;
      this.setState({
        isExpanded: IdOfLastTalkbackExpandedByUser === id || isAutoExpanded
      });
    }
  }, {
    key: "componentWillReceiveProps",
    value: function componentWillReceiveProps(nextProps) {
      this.setState({
        isExpanded: nextProps.isAutoExpanded
      });
    }
  }, {
    key: "render",
    value: function render() {
      var _this$state = this.state,
          isExpanded = _this$state.isExpanded,
          isAddCommentFormVisible = _this$state.isAddCommentFormVisible,
          commentFormType = _this$state.commentFormType,
          vote = _this$state.vote,
          talkback_like = _this$state.talkback_like,
          likes = _this$state.likes,
          unlikes = _this$state.unlikes;
      var _this$props4 = this.props,
          articleId = _this$props4.articleId,
          lang = _this$props4.lang,
          comment = _this$props4.comment,
          handleCommentsPopupClose = _this$props4.handleCommentsPopupClose,
          isAllCommentsPopupVisable = _this$props4.isAllCommentsPopupVisable;
      return this.props.render({

/***/ "../wcm_front-js/app/widgets/siteWidgets/videoPlayer/youtubePlayer/utils.js":
/***/ (function(module, exports, __webpack_require__) {

"use strict";


Object.defineProperty(exports, "__esModule", {
  value: true
});

exports.isYoutubeVideo = function (youtube_id, low_res_link) {
  return !low_res_link && youtube_id;
};

exports.attachYoutubeAnalytics = function (component, videoId) {
  var player = component.player;
  var container = component.youtubePlayerContainerAnchor;
  var analyticsLabel = document.URL + '&dcPath=' + window.dcPath + '&video=https://www.youtube.com/watch?v=' + videoId;
  var progressTimer;
  player.addEventListener("onStateChange", onPlayerStateChange);
  gaPushEvent(container.id, 'Load');
  var visibilityPercentage = getVisibilityPercentage();

  if (visibilityPercentage >= 50) {
    gaPushEvent(container.id, 'In-View');
  }

  function onPlayerStateChange(event) {
    var isPlay = 1 === event.data;
    var isPause = 2 === event.data;
    var isEnd = 0 === event.data;

    if (isPlay) {
      gaPushEvent(container.id, 'Play');
      progressTimer = setInterval(function () {
        ReportProgress();
      }, 1000);
      sendPopularityEvent(videoId, player.getDuration());
    }

    if (isPause) {
      gaPushEvent(container.id, 'Pause');
      clearTimeout(progressTimer);
    }

    if (isEnd) {
      ReportProgress();
      clearTimeout(progressTimer);
    }
  }

  function gaPushEvent(containerId, action) {
    window.dataLayer = window.dataLayer || [];
    window.dataLayer.push({
      'event': 'youtube_event',
      'Category': 'YouTube Player Events',
      'Action': 'YouTube Player ' + action,
      'Label': analyticsLabel
    });
  }

  function getVisibilityPercentage() {
    var windowHeight = document.documentElement.clientHeight;
    var docScroll = document.documentElement.scrollTop;
    var divPosition = container.offsetTop;
    var divHeight = container.clientHeight;
    var hiddenBefore = docScroll - divPosition;
    var hiddenAfter = divPosition + divHeight - (docScroll + windowHeight);

    if (docScroll > divPosition + divHeight || divPosition > docScroll + windowHeight) {
      return 0;
    } else {
      var result = 100;

      if (hiddenBefore > 0) {
        result -= hiddenBefore * 100 / divHeight;
      }

      if (hiddenAfter > 0) {
        result -= hiddenAfter * 100 / divHeight;
      }

      return Math.ceil(result);
    }
  }

  function sendPopularityEvent(videoId, videoTime) {
    if (!player.popularity) {
      var url = "//stats.ynet.co.il/popularity/popularity.php?content=".concat(videoId, "&type=1&time=").concat(videoTime, "&random=").concat(Math.floor(Math.random() * 999999));
      new Image().src = url;
      window.dataLayer.push({
        'event': 'youtube_event',
        'Category': 'YouTube Video',
        'Action': analyticsLabel,
        'Label': player.getVideoUrl() + ' & Play'
      });
      player.popularity = true;
    }
  }

  function ReportProgress() {
    var percentage = Math.round(player.getCurrentTime() / player.getDuration() * 100);
    var containerId = container.id;

    if (percentage > 0 && percentage <= 100) {
      if (percentage > 25 && percentage < 50 && !player.ga_25) {
        gaPushEvent(containerId, 'Quartiles 25%');
        player.ga_25 = true;
      } else if (percentage > 50 && percentage < 75 && !player.ga_50) {
        gaPushEvent(containerId, 'Quartiles 50%');
        player.ga_50 = true;
      } else if (percentage > 75 && percentage < 100 && !player.ga_75) {
        gaPushEvent(containerId, 'Quartiles 75%');
        player.ga_75 = true;
      } else if (percentage >= 100 && !player.ga_100) {
        gaPushEvent(containerId, 'Quartiles 100%');
        player.ga_100 = true;
      } else {
        return false;
      }
    }
  }
};

/***/ }),


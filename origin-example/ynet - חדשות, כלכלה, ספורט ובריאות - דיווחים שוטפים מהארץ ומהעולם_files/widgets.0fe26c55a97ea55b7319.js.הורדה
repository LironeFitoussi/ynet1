exports.getImageUrl = function (image, resize) {
  var url;
  var didCrop = false;

  if (typeof image == "string") {
    url = image;
  } else {
    if (!serverConfig_1.isWcmEditor() && !serverConfig_1.checkIfInServerPreviewMode()) {
      return frontImage_1.getFrontImageUrl(image, resize);
    }

    if (!image) {
      return "";
    }

    if (image.editedImageUrl) {
      return image.editedImageUrl;
    } else if (image.x === undefined || image.y === undefined || image.width === undefined || image.height === undefined) {
      url = image.url;
    } else {
      if (!image.url) return "";
      didCrop = true;
      url = "".concat(apiPrefix, "/service/image/edit/?url=").concat(image.url);
      if (image.rotate) url += "&rotate=".concat(image.rotate);

      if (image.width && image.height) {
        var imageCopy = Object.assign({}, image);
        if (imageCopy.x == null) imageCopy.x = 0;
        if (imageCopy.y == null) imageCopy.y = 0;
        url += "&crop=".concat(imageCopy.x, ",").concat(imageCopy.y, ",").concat(Number(imageCopy.x) + Number(imageCopy.width), ",").concat(Number(imageCopy.y) + Number(imageCopy.height));
      }

      if (image.compositeType) {
        url += "&compositeType=".concat(image.compositeType);
      }
    }
  }

  if (url && resize && (resize.h || resize.w)) {
    if (!didCrop) url = "".concat(apiPrefix, "/service/image/edit/?url=").concat(url);
    url += "&resize=".concat(resize.w || "", ",").concat(resize.h || "");
  }

  return url || "";
};


var getPollutionLevel = function getPollutionLevel(index) {
  if (index === null) {
    return null;
  }

  if (index > 50) {
    return "L";
  } else if (index < 51 && index > -1) {
    return "M";
  } else if (index < 0 && index > -201) {
    return "H";
  } else {
    return "D";
  }
};

var getDotColor = function getDotColor(index) {
  var pollution = getPollutionLevel(index);

  switch (pollution) {
    case "L":
      return {
        backgroundColor: "#62ac00"
      };
      break;

    case "M":
      return {
        backgroundColor: "#ecc500"
      };
      break;

    case "H":
      return {
        backgroundColor: "#e21818"
      };
      break;

    case "D":
      return {
        backgroundColor: "#8b2c09"
      };
      break;

    default:
      return {
        backgroundColor: "#707070"
      };
      break;
  }
};

var getRegionName = function getRegionName(regionId) {

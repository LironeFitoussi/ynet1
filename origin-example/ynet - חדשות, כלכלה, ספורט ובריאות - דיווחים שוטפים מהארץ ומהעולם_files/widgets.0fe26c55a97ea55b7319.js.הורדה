function attachPageRefrashUtils(api) {
  api.on("pause", function () {
    return siteWidgetUtils_1.enablePageRefresh();
  }).on("resume", function () {
    return siteWidgetUtils_1.disablePageRefresh();
  }).on("unload", function () {
    return siteWidgetUtils_1.enablePageRefresh();
  });
}

exports.attachPageRefrashUtils = attachPageRefrashUtils;

function attachPageRefreshUtilsYoutube(player) {
  player.addEventListener("onStateChange", function (event) {
    var player = event.target;

    switch (event.data) {
      case YT.PlayerState.PLAYING:
        siteWidgetUtils_1.disablePageRefresh();
        break;

      case YT.PlayerState.ENDED:
      case YT.PlayerState.PAUSED:
        siteWidgetUtils_1.enablePageRefresh();
        break;
    }
  });
}

exports.attachPageRefreshUtilsYoutube = attachPageRefreshUtilsYoutube;

function generateVideoId() {
  return "video_".concat(Math.random()).replace(".", "");
}

exports.generateVideoId = generateVideoId;

function shouldAutoPlayVideo(playerContainer, isAutoplayMarkedByUser) {
  return !window['cancelArticleVideoAutoplay'] && isAutoplayMarkedByUser && pageUrlContainsAutoplayHash() && isAboveTheFold(playerContainer) && isCorrectReferrer() && siteWidgetUtils_1.isPageNotReloaded();
}

exports.shouldAutoPlayVideo = shouldAutoPlayVideo;

function pageUrlContainsAutoplayHash() {
  return ~window.location.hash.indexOf(widgetsUtils_1.AUTOPLAY_HASH_TAG);
}

function isAboveTheFold(element) {
  //see http://ryanve.com/lab/dimensions/
  var videoElementTop = element.getBoundingClientRect().top;
  var videoElementBottom = element.getBoundingClientRect().bottom;
  var middleOfTheVideoElement = (videoElementBottom - videoElementTop) / 2;
  return element.getBoundingClientRect().top + middleOfTheVideoElement <= document.documentElement.clientHeight;
}

exports.isAboveTheFold = isAboveTheFold;

function isCorrectReferrer() {
  var referrer = document.referrer;

  if (referrer) {
    var a = document.createElement("a");
    a.href = referrer;
    return window.location.hostname === a.hostname;
  } else {
    return false;
  }
}


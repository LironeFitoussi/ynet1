function addDays(date, days) {
  var result = new Date(date);
  result.setDate(result.getDate() + days);
  return result;
}

function isHavakVisible(_ref) {
  var dateUpdatedOnSite = _ref.dateUpdatedOnSite,
      premiumExpireDays = _ref.premiumExpireDays;
  var dateUpdated = typeof dateUpdatedOnSite === "string" ? new Date(dateUpdatedOnSite) : dateUpdatedOnSite;

  if (!dateUpdated || !isFinite(dateUpdated) || !dateUpdated.getDate) {
    return 0;
  }

  if (premiumExpireDays) {
    var dateToExpire = addDays(dateUpdated, premiumExpireDays);
    return Number(dateToExpire > new Date());
  } else return 0;
}


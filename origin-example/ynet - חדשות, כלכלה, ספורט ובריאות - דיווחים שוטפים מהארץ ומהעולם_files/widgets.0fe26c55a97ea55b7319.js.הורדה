                });
              }).catch(function (err) {
                console.log('err', err);

                _this.setState({
                  loading: false
                });
              });
            });
          });
        }
      }
    };

    _this.handleChangeText = function (e) {
      var value = e.target.value;

      if (value.length === 0) {
        _this.setState({
          isTextAreaBlank: true
        });
      } else {
        _this.setState({
          isTextAreaBlank: false
        });
      }

      _this.setState({
        textArea: value
      });
    };

    _this.handleChangeExtraData = function (e) {
      var inputName = e.target.name;
      var value = e.target.value;
      var isInputValid = true;

      if (e.target.id === 'phoneNumber') {
        var pattern = /^[0-9\b]{10}$/;

        if (value) {
          isInputValid = pattern.test(value);
        }
      }

      if (e.target.id === 'mail') {
        var _pattern = /^((\w[^\W]+)[\.\-]?){1,}\@(([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3})|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

        if (value) {
          isInputValid = _pattern.test(value);
        }
      }

      if (e.target.id === "agreementApproved") {
        _this.setState(function (prevState) {
          return Object.assign({}, prevState, {
            isAgreementApproved: !prevState.isAgreementApproved
          });
        });
      }

      _this.setState(function (prevState) {
        var _ref;

        return _ref = {}, _defineProperty(_ref, inputName, value), _defineProperty(_ref, "validation", Object.assign({}, prevState.validation, _defineProperty({}, inputName, isInputValid))), _ref;
      });
    };

    _this.onClose = function () {
      _this.resetInputs();
    };

    _this.handleNumOfFiles = function (action) {
      switch (action) {
        case "add":
          _this.setState(function (prevState) {
            return Object.assign({}, prevState, {
              numOfFiles: prevState.numOfFiles + 1
            });
          });

          break;

        case "remove":
          _this.setState(function (prevState) {
            return Object.assign({}, prevState, {
              numOfFiles: prevState.numOfFiles - 1
            });
          });

          break;

        default:
          null;
      }
    };

    _this.resetInputs = function () {

    _this.createAWS3Script = function () {
      var script = document.createElement("script");
      script.src = 'https://sdk.amazonaws.com/js/aws-sdk-2.945.0.min.js';
      script.type = "text/javascript";
      script.id = "amazonS3";
      document.body.appendChild(script);
    };

    _this.sendS3 = function (filesData, file) {
      var _filesData$policy = filesData.policy,
          AccessKeyId = _filesData$policy.AccessKeyId,
          SecretAccessKey = _filesData$policy.SecretAccessKey,
          SessionToken = _filesData$policy.SessionToken;
      AWS.config.update({
        region: filesData.region,
        credentials: new AWS.Credentials(AccessKeyId, SecretAccessKey, SessionToken)
      });
      AWS.config.apiVersions = {
        s3: '2006-03-01'
      };
      _this.s3 = new AWS.S3();
      var key = "".concat(filesData.key, "_").concat(Math.random().toString().substr(2), "_").concat(file.name.replace(/\s/g, ''));
      var uploadParams = {
        Bucket: filesData.bucket,
        Key: key,
        ContentType: file.type
      };

      _this.s3.createMultipartUpload(uploadParams, function (err, data) {
        if (err) {
          console.error("S3 Upload error", err);
        }

        if (data) {
          _this.setSubmitButtonDisabled;

          _this.setState({
            selectedFiles: Object.assign({}, _this.state.selectedFiles, _defineProperty({}, file["key"], {
              fileName: file.name,
              progress: 0,
              uploadKey: data.Key
            }))
          });

          _this.uploadS3Part(data.UploadId, filesData.bucket, 1, Math.ceil(file.size / MAX_PART_SIZE), file, key, []);
        }
      });
    };

    _this.uploadS3Part = function (uploadid, bucket, partNum, totalParts, file, key, etags) {
      var start = (partNum - 1) * MAX_PART_SIZE;
      var end = partNum * MAX_PART_SIZE; //file slice dosent reach beyond the size of the file

      var params = {
        Body: file.slice(start, end),
        Bucket: bucket,
        Key: key,
        PartNumber: partNum,
        UploadId: uploadid
      }; //https://docs.aws.amazon.com/AWSJavaScriptSDK/latest/AWS/S3.html#uploadPart-property

      _this.s3.uploadPart(params, function (err, data) {
        if (err) {
          var existingFiles = _this.state.selectedFiles;
          delete existingFiles[file["key"]].progress;

          _this.setState({
            selectedFiles: existingFiles
          });

          _this.isUploadFinished();

          console.error(err, err.stack);
        } else {
          etags.push(data.ETag);

          if (partNum !== totalParts) {
            _this.setState({
              selectedFiles: Object.assign({}, _this.state.selectedFiles, _defineProperty({}, file["key"], {
                fileName: file.name,
                progress: partNum * 100 / totalParts,
                uploadKey: key
              }))
            });

            _this.uploadS3Part(uploadid, bucket, partNum + 1, totalParts, file, key, etags);
          } else {
            _this.finishS3Part(bucket, uploadid, key, etags);
          }
        }
      });
    };

    _this.finishS3Part = function (bucket, uploadId, key, etags) {
      var params = {
        Bucket: bucket,
        Key: key,
        MultipartUpload: {
          Parts: etags.map(function (eTag, index) {
            return {
              ETag: eTag,
              PartNumber: index + 1
            };
          })
        },
        UploadId: uploadId
      }; //https://docs.aws.amazon.com/AWSJavaScriptSDK/latest/AWS/S3.html#completeMultipartUpload-property

      _this.s3.completeMultipartUpload(params, function (err, data) {
        if (err) {
          console.error(err, err.stack);
        } else {
          _this.handleNumOfFiles("add");

          Object.keys(_this.state.selectedFiles).map(function (fileKey, index) {
            if (_this.state.selectedFiles[fileKey].uploadKey === data.Key) {
              var existingFiles = _this.state.selectedFiles;
              delete existingFiles[fileKey].progress;

              _this.setState({
                selectedFiles: existingFiles
              });
            }
          });

          _this.isUploadFinished();
        }

        ;
      });
    };

    _this.manageFileUpload = function (policy, file) {
      var filesData = policy;
      var oldHashesArray = _this.state.hashes;

      _this.sendS3(filesData, file);

      _this.setState({
        hashes: [].concat(_toConsumableArray(oldHashesArray), [policy.hash])
      });

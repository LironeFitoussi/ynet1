  if (adsElement) {
    var anchorElement = document.createElement("a");
    anchorElement.className = "ynetfp-ads fp-icon";

    anchorElement.onclick = function () {
      return api.unload();
    };

    adsElement.appendChild(anchorElement);
  }
}


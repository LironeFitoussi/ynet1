              });
            });
          });
        }
      });
    };

    _this.handleSelectedFile = function (e) {
      var files = e.target.files;
      var siteSettingSitename = _this.props.siteSettingSitename;

      if (files.length > 5 || files.length + Object.keys(_this.state.selectedFiles).length > 5) {
        alert(maxFileAlertMessage[siteSettingSitename]);
      } else {
        var validFiles = _this.isFilesSizeValid(files);

        if (!_this.state.s3Policy) {
          _this.getS3Policy().then(function (res) {
            _this.sendFiles(validFiles);
          });
        } else {
          _this.sendFiles(validFiles);
        }
      }
    };

    _this.handleRemoveFile = function (index) {

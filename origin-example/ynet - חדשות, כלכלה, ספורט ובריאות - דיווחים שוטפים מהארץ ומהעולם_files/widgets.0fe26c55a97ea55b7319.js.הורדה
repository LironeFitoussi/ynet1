  "&quot;": '"',
  "&#39;": "'",
  "&amp;": "&",
  "&lt;": "<",
  "&gt;": ">"
};
var htmlReplacementsRegexString = Object.keys(htmlReplacements).join("|");
var htmlReplacementsRegex = new RegExp(htmlReplacementsRegexString, "g");

function removeHtmlTagsFromString(input) {
  if (input) {
    return input.replace(/<(?:.|\n)*?>/gm, '').replace(/&[n]bsp;/g, ' ').replace(htmlReplacementsRegex, function (s) {
      return htmlReplacements[s];
    }).trim();
  }

  return "";
}

exports.removeHtmlTagsFromString = removeHtmlTagsFromString;


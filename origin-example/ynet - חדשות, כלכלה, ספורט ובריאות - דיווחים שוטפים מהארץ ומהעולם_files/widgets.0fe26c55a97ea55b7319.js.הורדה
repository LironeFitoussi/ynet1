function dateToString(d) {
  if (!d) return null;
  d = typeof d === "string" ? new Date(d) : d;
  return "".concat(zeroPad2(d.getDate()), "/").concat(zeroPad2(d.getMonth() + 1), "/").concat(d.getFullYear(), " ").concat(zeroPad2(d.getHours()), ":").concat(zeroPad2(d.getMinutes()));
}

exports.dateToString = dateToString;

function zeroPad2(n) {
  if (n < 0) throw "".concat(n, " is less then zero and cannot be padded");
  return n < 10 ? "0".concat(n) : n.toString();
}

function bool2number(b) {
  return b ? 1 : 0;
}

exports.bool2number = bool2number;

function stringToNumber(s) {
  return Number(s);
}

exports.stringToNumber = stringToNumber;

